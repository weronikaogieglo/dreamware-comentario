{
    "name": "comentario-frontend",
    "version": "3.0.0",
    "private": true,
    "scripts": {
        "build": "ng build --configuration development",
        "build:prod": "ng build",
        "generate": "rm -rf generated-api/ && openapi-generator-cli generate -i ../resources/swagger/swagger.yml -g typescript-angular -o generated-api",
        "lint": "eslint",
        "start": "ng build --watch --configuration development",
        "test": "ng test",
        "test:ci": "ng test --browsers=ChromeHeadlessCI --watch=false --progress=false",
        "copy-assets": "cp ../build/frontend/en/*.ico ../build/frontend/en/*.png ../build/frontend/en/*.svg ../build/frontend/en/*.xml ../build/frontend/en/*.txt ../build/frontend/en/site.webmanifest ../build/frontend/",
        "cleanup-assets": "rm -f ../build/frontend/*/*.ico ../build/frontend/*/*.png ../build/frontend/*/*.svg ../build/frontend/*/*.xml ../build/frontend/*/*.txt ../build/frontend/*/site.webmanifest",
        "postbuild": "npm run copy-assets && npm run cleanup-assets",
        "postbuild:prod": "npm run copy-assets && npm run cleanup-assets"
    },
    "dependencies": {
        "@angular/animations": "^19.2.14",
        "@angular/common": "^19.2.14",
        "@angular/compiler": "^19.2.14",
        "@angular/core": "^19.2.14",
        "@angular/forms": "^19.2.14",
        "@angular/localize": "^19.2.14",
        "@angular/platform-browser": "^19.2.14",
        "@angular/platform-browser-dynamic": "^19.2.14",
        "@angular/router": "^19.2.14",
        "@fortawesome/angular-fontawesome": "^1.0.0",
        "@fortawesome/fontawesome-svg-core": "^6.7.2",
        "@fortawesome/free-brands-svg-icons": "^6.7.2",
        "@fortawesome/free-regular-svg-icons": "^6.7.2",
        "@fortawesome/free-solid-svg-icons": "^6.7.2",
        "@ng-bootstrap/ng-bootstrap": "^18.0.0",
        "@ngneat/until-destroy": "^10.0.0",
        "@popperjs/core": "^2.11.8",
        "bootstrap": "^5.3.6",
        "chart.js": "^4.4.9",
        "ng2-charts": "^8.0.0",
        "ngx-highlightjs": "^14.0.0",
        "rxjs": "^7.8.2",
        "tslib": "^2.8.1",
        "zone.js": "~0.15.1"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^19.2.14",
        "@angular-devkit/core": "^19.2.14",
        "@angular-devkit/schematics": "^19.2.14",
        "@angular/cli": "^19.2.14",
        "@angular/compiler-cli": "^19.2.14",
        "@openapitools/openapi-generator-cli": "^2.20.2",
        "@types/jasmine": "^5.1.8",
        "angular-eslint": "^19.7.1",
        "jasmine-core": "^5.7.1",
        "karma": "^6.4.4",
        "karma-chrome-launcher": "^3.2.0",
        "karma-coverage": "^2.2.1",
        "karma-jasmine": "^5.1.0",
        "karma-jasmine-html-reporter": "^2.1.0",
        "ng-mocks": "^14.13.5",
        "vite": "^6.3.5"
    },
    "peerDependencies": {
        "@angular/cdk": "^19.2.18"
    }
}
